				"description": "The page to get",
				"required": false,
				"schema": {
					"type": "integer",
					"default": 1,
					"minimum": 0,
					"exclusiveMinimum": true
				}
			},
			"ListPageSize": {
				"name": "x-janis-page-size",
				"in": "header",
				"description": "The page size to get",
				"required": false,
				"schema": {
					"type": "integer",
					"default": 60,
					"minimum": 0,
					"exclusiveMinimum": true
				}
			},
			"JanisEntity": {
				"name": "janis-entity",
				"in": "header",
				"description": "The entity name",
				"required": true,
